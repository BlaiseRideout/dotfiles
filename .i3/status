general {
	output_format = "i3bar"
	colors = false
	interval = 5
}

order += "wireless wlan0"
order += "battery 0"
order += "tztime local"

wireless wlan0 {
	format_up = "W: %essid (%quality)"
	format_down = ""
}

battery 0 {
        format = "%status %percentage %emptytime"
	format_down = ""
	low_threshold = 10
	#hide_seconds = true
	integer_battery_capacity = true
}

tztime local {
	format = "%Y-%m-%d %H:%M"
}

